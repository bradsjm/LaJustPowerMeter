<UserControl x:Class="GameEngine.RoundNumber"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:transc="clr-namespace:Transitionals.Controls;assembly=Transitionals"
    xmlns:transt="clr-namespace:Transitionals.Transitions;assembly=Transitionals">

    <transc:TransitionElement x:Name="RoundNumberBox" TransitionsEnabled="False">
        <transc:TransitionElement.Transition>
            <transt:FadeAndGrowTransition />
        </transc:TransitionElement.Transition>
        <Image Stretch="Uniform" Source="/GameEngine;component/Resources/Rounds/0R.png"/>
    </transc:TransitionElement>
</UserControl>
