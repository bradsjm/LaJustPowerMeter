<UserControl x:Class="LaJust.PowerMeter.Modules.GameEngine.Views.GameRoundView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:uc="clr-namespace:LaJust.PowerMeter.Modules.GameEngine.UserControls"
    Background="Black">
    <Border BorderThickness="3" CornerRadius="5" BorderBrush="LightSlateGray" Background="DarkSlateGray">
        <uc:RoundNumber Round="{Binding Game.RoundNumber}"/>
    </Border>
</UserControl>
